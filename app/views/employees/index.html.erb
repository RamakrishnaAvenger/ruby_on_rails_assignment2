<% @lead_names.each do |lead_name| %>
Team Lead: <%= lead_name %>
<br><br>
<table>
  <tr>
    <th>Name</th>
    <th>Skills</th>
    <th colspan="2"></th>
  </tr>
  <% @employees[lead_name].each do |employee_data| %>
    <tr>
      <td><%= employee_data[:name] %></td>
      <td><%= employee_data[:skills] %></td>
    </tr>
  <% end %>
</table>
<br><br>
<% end %>
<br><br>

<%= link_to 'Add New Employee', new_employee_path %>